    "div4" : {
        "type" : "separator"
    },
    "headcolor" : {
        "type" : "header",
        "description" : "Coloration Adjustments: "
    },
    "vibrancy" : {
        "type": "scale",
        "description": "Darken Accent Color",
        "min": 0,
        "max": 1,
        "default": 0.5,
        "step": 0.1
    },
    "mainshade" : {
        "type" : "checkbox",
        "description" : "Choose Main Shade In Wallpaper",
        "default" : false
    },
    "flowcolors" : {
        "type" : "checkbox",
        "description" : "Color Flow - GTK",
        "default" : true,
        "tooltip" : "When active, DermoDeX will make an attempt to transfer the hover color from the Applictaions Menu into GTK. Set this on or off depending on your results. If DermoDeX detects a shade that is too dark for GTK it will attempt avoiding flowing it through."
    },
    "flowcolorsmenu" : {
        "type" : "checkbox",
        "description" : "Color Flow - Menu Launcher",
        "default" : true,
        "tooltip" : "When active, DermoDeX transfer the accent color to the Applictaions Menu button on click."
    },
    
    "flowsidebar" : {
        "type" : "checkbox",
        "description" : "Color Flow - Sidebar",
        "default" : true,
        "tooltip" : "When active, DermoDeX will make an attempt to shade the sidebar for example in the nemo file manager."
    },
    "div1" : {
        "type" : "separator"
    },
    "headoverrides" : {
        "type" : "header",
        "description" : "Color Overrides: "
    },
    "coloroverrides" : {
        "type" : "checkbox",
        "description" : "Active",
        "default" : false,
        "tooltip" : "Override DermoDex's chosen colors with your own selections"
    },
    "override0": {
        "type": "colorchooser",
        "default": "aN",
        "dependency" : "coloroverrides",
        "description": "Override Shade 0"
    },
    "override1": {
        "type": "colorchooser",
        "default": "aN",
        "dependency" : "coloroverrides",
        "description": "Override Shade 1"
    },
    "override2": {
        "type": "colorchooser",
        "default": "aN",
        "dependency" : "coloroverrides",
        "description": "Override Shade 2"
    },
    "div2" : {
        "type" : "separator"
    },
    "headcinn" : {
        "type" : "header",
        "description" : "Cinnamon Tweaks: "
    },
    "paneltrans" : {
        "type": "scale",
        "description": "Panel Transparancy",
        "min": 0,
        "max": 1,
        "default": 1.0,
        "step": 0.1
    },
    "panelstyle" : {
        "type": "combobox",
        "default" : "flat",
        "description" : "Panel Style ",
        "options" : {
            "Modern (Rounded UI)" : "modern",
            "Flat (Windows UI)" : "flat"
        },
        "tooltip" : "There are two panel classes in Cinnamon, an outer layer known as the Panel and then a Panel Left, Center and Right. Modern rounded styles apply to the inner panel segments."
    },
    "panelshade" : {
        "type": "combobox",
        "default" : "dark",
        "description" : "Panel Shade ",
        "options" : {
            "Light" : "light",
            "Medium" : "medium",
            "Dark" : "dark",
            "Darkest" : "darkest"
        },
        "tooltip" : "When setting the overall panel shade when no background image blur is being used, a dark, medium or light shade can be given to the panel."
    },
    "panellocat" : {
        "type": "combobox",
        "default" : "top",
        "description" : "Panel Location ",
        "options" : {
            "Bottom" : "bottom",
            "Top" : "top",
            "Left" : "left",
            "Right" : "right"
        },
        "tooltip" : "For the background image blur to be effective, set the location of where the main panel normally lives"
    },
    "panelblur" : {
        "type" : "checkbox",
        "description" : "Panel Blur",
        "default" : false
    },
    "menubckgrd" : {
        "type" : "checkbox",
        "description" : "Menu Blur",
        "default" : false
    },
    "menutrans" : {
        "type": "scale",
        "description": "Menu Transparancy",
        "min": 0.5,
        "max": 1,
        "default": 0.9,
        "step": 0.1,
        "tooltip": "Use this when the menu blur is off, don't go too crazy below 0.5 things can get hard to read."
    },
    "pblur": {
        "type": "spinbutton",
        "default": 100,
        "min": 50,
        "max": 150,
        "units": "pixels",
        "step": 1,
        "description": "Panel Blur Amount",
        "tooltip": "Tweak the amount of blur for the panel and menu"
    },
    "lblur": {
        "type": "spinbutton",
        "default": 100,
        "min": 50,
        "max": 150,
        "units": "pixels",
        "step": 1,
        "description": "Login Blur Amount",
        "tooltip": "Tweak the amount of blur for the login screen (not the lock screen)"
    },
    "menuavatar" : {
        "type" : "checkbox",
        "description" : "Menu User Picture",
        "default" : true
    },
    
    "div3" : {
        "type" : "separator"
    },
    "headimage" : {
        "type" : "header",
        "description" : "Image Processing Tweaks: "
    },
    "colorcollect": {
        "type": "spinbutton",
        "default": 8,
        "min": 5,
        "max": 13,
        "units": "swatches",
        "step": 1,
        "description": "Color Collect",
        "tooltip": "To allow for variation in found colors in complex wallpapers, increases the chance of isolating a lesser occuring pop of color"
    },
    "splitimage": {
        "type": "spinbutton",
        "default": 2,
        "min": 2,
        "max": 4,
        "units": "segments",
        "step": 1,
        "description": "Split Image",
        "tooltip": "To speed up image processing and to help isolate a focal point in the wallpaper"
    },
    "splitfocus": {
        "type": "spinbutton",
        "default": 2,
        "min": 1,
        "max": 4,
        "units": "segment",
        "step": 1,
        "description": "Split Image Focus",
        "tooltip": "Choose which segment to focus on, for example spliting the image into 2 segments either focus on the first or second segment"
    },
    "splitdirection" : {
        "type": "combobox",
        "default" : "v",
        "description" : "Split Image Direction ",
        "options" : {
            "Vertical" : "v",
            "Horizontal" : "h"
        },
        "tooltip" : "Split the wallpaper into segments sideways (Vertical) or from top to bottom (Horizontal)"
    },
    "saturation" : {
        "type": "scale",
        "description": "Image Saturation",
        "min": 1,
        "max": 1.5,
        "default": 1.1,
        "step": 0.1
    },
    "brightness" : {
        "type": "scale",
        "description": "Image Brightness",
        "min": 1,
        "max": 1.5,
        "default": 1.1,
        "step": 0.1
    },
    "contrast" : {
        "type": "scale",
        "description": "Image Contrast",
        "min": 1,
        "max": 1.5,
        "default": 1.1,
        "step": 0.1
    }
}